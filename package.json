{
  "name": "@dragonlabs/react-ui-components",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "version": "0.0.4",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "// root commands": "//",
    "prod": "run-s clean build-prod",
    "test": "run-s clean build-test start-test",
    "// entry points": "//",
    "start-test": "cross-env NODE_ENV=test nyc mocha -r ./src/test-utils/jsdom-setup.js \"./dist/**/*.js\"",
    "// root build jobs": "//",
    "build-prod": "run-s build-webpack-prod",
    "build-test": "run-s build-webpack-test",
    "// inner build jobs": "//",
    "build-webpack-prod": "cross-env NODE_ENV=production webpack -p",
    "build-webpack-test": "cross-env NODE_ENV=test webpack",
    "// linters": "//",
    "lint": "run-s lint-src lint-style",
    "lint-fix": "run-s lint-src-fix lint-style-fix",
    "lint-src": "eslint --ext ts,tsx,js,jsx src",
    "lint-src-fix": "eslint --fix --ext ts,tsx,js,jsx src",
    "lint-style": "prettier -c \"src/**/*.{css,scss}\"",
    "lint-style-fix": "prettier --write \"src/**/*.{css,scss}\"",
    "// misc": "//",
    "bundlewatch": "bundlewatch --config ./.bundlewatch/config.json",
    "clean": "rimraf ./dist ./coverage ./.nyc_output"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.2.3",
    "@types/chai-string": "^1.4.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.7.9",
    "@types/react": ">=16.3.0",
    "@types/react-dom": ">=16.3.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "bundlewatch": "^0.2.5",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "cross-env": "^6.0.3",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "glob": "^7.1.4",
    "jsdom": "^16.2.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.18.2",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "yarn-deduplicate": "^2.0.0"
  },
  "dependencies": {
    "bootstrap-scss": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  }
}
